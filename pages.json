{
	"easycom": {
		"^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "蓝牙信息",
				"enablePullDownRefresh": true
			}
		},
		{
			"path": "pages/echarts/echarts",
			"style": {
				"navigationBarTitleText": "曲线图表"
			}

		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "white",
		"navigationBarTitleText": "久用蓝牙连接测算",
		"navigationBarBackgroundColor": "#409eff",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar": {
		"color": "#909399",
		"backgroundColor": "#FFFFFF",
		"borderStyle": "black",
		"selectedColor": "#1296db",
		"list": [
			{
				"pagePath": "pages/index/index",
				"iconPath": "static/icon/1.png",
				"selectedIconPath": "static/icon/11.png",
				"text": "蓝牙"
			},
			{
				"pagePath": "pages/echarts/echarts",
				"iconPath": "static/icon/2.png",
				"selectedIconPath": "static/icon/22.png",
				"text": "曲线图"
			}
		]
	}
}
